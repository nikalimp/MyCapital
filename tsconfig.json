{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["react", "react-native"]
  },
  "include": ["**/*.ts", "**/*.tsx", "types/**/*.d.ts"]
}
